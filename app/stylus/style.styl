@font-face {
    font-family: 'LCDNovaObl';
    src: url('fonts/alcdnovaoblallfontru.eot');
    src: url('fonts/alcdnovaoblallfontru.eot?#iefix') format('embedded-opentype'),
        url('fonts/alcdnovaoblallfontru.woff2') format('woff2'),
        url('fonts/alcdnovaoblallfontru.woff') format('woff'),
        url('fonts/alcdnovaoblallfontru.ttf')  format('truetype'),
        url('fonts/alcdnovaoblallfontru.svg#a_LCDNovaObl') format('svg');
        font-weight: normal;
        font-style: normal;
}

@font-face {
    font-family: 'Cyber';
    src: url('fonts/BlenderPro-Bold.eot');
    src: url('fonts/BlenderPro-Bold.eot?#iefix') format('embedded-opentype'),
        url('fonts/BlenderPro-Bold.svg#BlenderPro-Bold') format('svg'),
        url('fonts/BlenderPro-Bold.ttf') format('truetype'),
        url('fonts/BlenderPro-Bold.woff') format('woff'),
        url('fonts/BlenderPro-Bold.woff2') format('woff2');
    font-weight: normal;
    font-style: normal;
}

.preloader
    position fixed
    left 0
    top 0
    right 0
    bottom 0
    background #9cab84
    z-index 1001

    img
        position absolute
        left 50%
        top 50%
        transform translate3d(-50%, -50%, 0)

.loaded
    &_hiding
        .preloader
            transition 0.3s opacity
            opacity 0

    .preloader
        display none

html
    overflow hidden
    margin 0
    overflow hidden
    position fixed
    height 100%
    width 100%

body
    margin 0
    transition background .1s
    overflow hidden
    position fixed
    height 100%
    width 100%

.header, .main
    max-width 1440px
    margin-left auto
    margin-right auto
    padding-left .5em
    padding-right .5em

.header
    padding-top .5em
    &__menu
        margin-bottom 1em

.main
    &__settings
        position absolute
        width 100%
        height 100%
        left 0
        top 0

.clearfix
    zoom 1

    &::before, &::after
        content ""
        display table

    &::after
        clear both

.ndmx
    text-align center
    width 100%
    position absolute
    bottom 0
    opacity .3
    transition opacity 0.5s

    &:hover
        opacity 1

.menu
    text-align right

    &__button
        &:first-child
            float left

.button
    outline none
    border none
    cursor pointer

.button__glitch, .button__sub
    display none

.info
    text-align center

    &__score
        display inline-block
        margin-left auto
        vertical-align top

    &__overText, &__winText
        display inline-block
        white-space nowrap
        opacity 0
        width 0
        text-align right
        transition width 1s, opacity 0.1s, color 0.3s

    &__overText
        overflow hidden

        &_visible
            opacity 1

    &__winText
        overflow hidden

        &_visible
            opacity 1

    &__wakeUpText
        display none

.timerDark
    position absolute
    width 100%
    height 1.25em
    left 50%
    transform translate3d(-50%, 0, 0)
    transition width .25s ease

    &_run
        animation-fill-mode forwards
        animation-timing-function ease-out

@keyframes timerDarkRunTetris
    0%
        background-color #7ba636
        width 100%
    10%
        background-color #7ba636
    100%
        background-color #d50000
        width 0

@keyframes timerDarkRunCyber
    0%
        background-color #00f0ff
        width 100%
        border-left-width 2px
        border-right-width 2px
    10%
        background-color #00f0ff
    100%
        background-color #ff003c
        width 0
        border-left-width 0
        border-right-width 0

.score
    &__count
        display inline-block
        width 1px
        text-align left

.field
    position relative
    max-width 100%
    margin-left auto
    margin-right auto

    &__table
        position relative
        width 100%
        padding-bottom 100%

    &__timer
        position absolute
        z-index 613

    &__dark
        position absolute
        z-index 611
        opacity 0
        visibility hidden

        &_visible
            visibility visible
            animation-name darkVisible
            animation-fill-mode forwards

@keyframes darkVisible
    0%
        opacity 0
    10%
        opacity 0
    100%
        opacity 1

.table
    position absolute
    left 0
    top 0
    width 100%
    height 100%
    display -webkit-flex
    display -moz-flex
    display -ms-flex
    display -o-flex
    display flex
    justify-content space-between
    flex-direction column
    flex-grow 1
    flex-shrink 1
    box-sizing border-box

    &__line
        width 100%
        height 100%
        display -webkit-flex
        display -moz-flex
        display -ms-flex
        display -o-flex
        display flex
        justify-content space-between
        flex-direction row
        flex-grow 1
        flex-shrink 1

    &__cell
        width 100%

.cyberBackground
    display none

.timer
    text-align center
    display none

    &_start
        display block

    &__count
        position absolute
        width 100%
        top 50%
        left 50%
        transform translate3d(-50%, -50%, 0)
        z-index 614
        font-size 4em

.settings
    visibility hidden
    opacity 0
    transition visibility 0s .1s, opacity .1s linear

    .button
        opacity 0
        transform translate3d(0, 5em, 0) scale(.9)

    &_open
        visibility visible
        opacity 1
        transition opacity .1s linear
        z-index 701

        .form__item
            &:nth-child(1), &:nth-child(2), &:nth-child(3)
                opacity 1
                transform translate3d(0, 0, 0) scale(1)

        .button
            opacity 1
            transform translate3d(0, 0, 0) scale(1)

    &__form
        position absolute
        left 50%
        top 50%
        transform translate3d(-50%, -50%, 0)
        width 90%
        max-width 950px

    input
        border-radius 0
        position absolute
        z-index -1
        opacity 0
        left 0
        top 0

        &+label
            display inline-flex
            align-items center
            user-select none
            position relative
            padding-left 0.51em
            padding-right 0.4em
            padding-top 0.2em
            padding-bottom 0.2em
            transition all 0.2s, outline-offset 0.5s
            outline 1px solid transparent
            outline-offset 1em

            &::before
                content ''
                display inline-block
                width 0.5em
                height 0.5em
                flex-shrink 0
                flex-grow 0
                outline 1px solid rgba(0, 0, 0, 0.6)
                outline-offset 0.11em
                margin-right 0.5em
                background-repeat no-repeat
                background-position center center
                background-size 50% 50%
                transition all 0.2s

        &:checked+label:before
            outline 1px solid black
            background-color black

    label
        margin-right .5em
        font-size 1em
        cursor pointer

        &:last-of-type
            margin-right 0

.form
    text-align center
    font-size 2em

    &__item
        margin-bottom .5em
        text-align center
        opacity 0
        transform translate3d(0, -1em, 0) scale(.9)

        legend
            text-align center

        &:nth-child(1)
            transition all .2s ease .1s

        &:nth-child(2)
            transition all .2s ease .2s

        &:nth-child(3)
            transition all .2s ease .3s

fieldset>legend
    display table
    float none
    margin 0 auto

.touchControl
    position absolute
    top 0
    left 0
    right 0
    bottom 0
    z-index 612

    &_none
        display none

    &__item
        position absolute
        top -1em
        left -1em
        bottom -1em
        right -1em
        background-color rgba(0, 0, 0, 0.025)
        transition transform .1s ease
        outline none
        border none

        &.touch
            background-color rgba(0, 0, 0, 0.05)

        &_top
            clip-path polygon(0 0, 50% 50%, 100% 0)
            transform translate3d(0, -0.55em, 0)

            &.touch
                transform translate3d(0, -1.5em, 0)

        &_right
            clip-path polygon(100% 0, 50% 50%, 100% 100%)
            transform translate3d(0.55em, 0, 0)

            &.touch
                transform translate3d(1.5em, 0, 0)

        &_bottom
            clip-path polygon(0 100%, 50% 50%, 100% 100%)
            transform translate3d(0, 0.55em, 0)

            &.touch
                transform translate3d(0, 1.5em, 0)

        &_left
            clip-path polygon(0 0, 50% 50%, 0 100%)
            transform translate3d(-0.55em, 0, 0)

            &.touch
                transform translate3d(-1.5em, 0, 0)

@media screen and (-moz-images-in-menus: 0)
    fieldset
        position relative
    
    fieldset>legend
        position absolute
        left 50%
        top -12px
        background white
        transform translate3d(-50%, 0, 0)
    
    .cyberpunkRange
        bottom 10px !important

@keyframes rotateAndScale
    0%
        transform rotate(0) scale(1)
    50%
        transform rotate(90deg) scale(.8)
    100%
        transform rotate(-90deg) scale(1)

@keyframes flashing
    0%
        opacity 0.1
    50%
        opacity 0.4
    100%
        opacity 0.1

@keyframes shake
    0%
        transform translate3d(1px, 1px, 0) rotate(0deg)
    10%
        transform translate3d(-1px, -2px, 0) rotate(-1deg)
    20%
        transform translate3d(-3px, 0px, 0) rotate(1deg)
    30%
        transform translate3d(3px, 2px, 0) rotate(0deg)
    40%
        transform translate3d(1px, -1px, 0) rotate(1deg)
    50%
        transform translate3d(-1px, 2px, 0) rotate(-1deg)
    60%
        transform translate3d(-3px, 1px, 0) rotate(0deg)
    70%
        transform translate3d(3px, 1px, 0) rotate(-1deg)
    80%
        transform translate3d(-1px, -1px, 0) rotate(1deg)
    90%
        transform translate3d(1px, 2px, 0) rotate(0deg)
    100%
        transform translate3d(1px, -2px, 0) rotate(-1deg)

@keyframes crash
