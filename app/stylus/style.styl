@keyframes win
    0%
        background-color black
        transform scale(1) rotate(0)
        outline 0px solid black
        outline-offset 0px
    50%
        background #b0c68c
        background-color black
        transform scale(.5) rotate(360deg)
        outline 20px solid black
        outline-offset 20px
    100%
