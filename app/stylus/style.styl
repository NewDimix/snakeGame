.tetris
    background-color #9cab84
    font-family LCDNovaObl
    font-size 10px

    &::before
        content ''
        font-family Cyber

    .button 
        font-family LCDNovaObl
        font-size 2em
        color #9cab84
        background-color black
        position relative
        transition height 1s, background-color 1s, outline .2s, outline-offset .2s
        padding-top .2em
        padding-bottom .2em
        width 6.5em
        outline 1px solid transparent
        outline-offset .5em

        &:hover, &:focus
            outline 1px solid rgba(0, 0, 0, 0.4)
            outline-offset .15em

            &::after
                height 100% !important
                opacity 1
                color #FFF
                background-color rgba(156, 171, 132, 0.2)

        &::after
            content ''
            position absolute
            width 100%
            height 0
            left 50%
            top 50%
            transform translate3d(-50%, -50%, 0)
            border 1px solid #9cab84
            opacity 0
            transition all .5s

    .info
        font-size 2.3em
        transform translate3d(0, -.2em, 0)

        &__overText_visible, &__winText_visible
            width 6.7em
            padding-right .15em
            text-shadow .05em 0 0 rgba(0, 0, 0, 0.4),
            0 .05em 0 rgba(0, 0, 0, 0.4),
